js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/site/Footer.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/site/Header.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/site/Sidebar.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/site/Home.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/site/Resources.js",["81","82","83","84","85","86","87","88","89","90"],"import React from 'react';\n\nconst Resources = () => {\n    return (\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <h1>React Resources</h1>\n                <p>Below are some of the resources that we used throughout this app. Note: You'll have one challenge where you will be asked to make this page better.</p>\n                <ul>\n                    <li>\n                       <a href=\"https://reactjs.org/docs/getting-started.html\" target=\"_blank\">\n                          The Official Docs\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://reacttraining.com/react-router\" target=\"_blank\">\n                          React Router Dom\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://www.udemy.com/course/react-redux/\" target=\"_blank\">\n                          Stephen Grider's Udemy Course for the Udemy API\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://www.pluralsight.com/course/react-js-getting-started\" target=\"_blank\">\n                          Samer Buna's Pluralsight Course for the GitHub API Starter\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://www.udemy.com/course/react-the-guide-incl-redux\" target=\"_blank\">\n                          Maximilian Schwarzmüller's Udemy Course\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://reactstrap.github.io/\" target=\"_blank\">\n                          ReactStrap for a Bootstrap Application\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://gorangajic.github.io/react-icons/fa.html\" target=\"_blank\">\n                          Font Awesome for React\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://github.com/bmorelli25/interactive-bitcoin-price-chart\" target=\"_blank\">\n                          Bitcoin\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://github.com/mthorry/earthquakes-mapper\" target=\"_blank\">\n                          Google Maps\n                       </a>\n                    </li>\n                    <li>\n                       <a href=\"https://neptunian.github.io/react-photo-gallery\" target=\"_blank\">\n                          React Photo Gallery\n                       </a>\n                    </li>\n               </ul>\n            </div>\n        </div>\n    );\n}\nexport default Resources;","/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/concepts/FunctionalComponentDemo.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/concepts/JSXRules.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/concepts/State.js",["91"],"import React, {useState} from 'react';\n\nconst State = () => {\n    return (\n        <div className='main'>\n            <div className='mainDiv'>\n                <ul>\n                    <dt>useState is Unique to Functional Components</dt>\n                    <dd>Class components have a separate meaning of using and changing state.</dd>\n                    <dt>useState Uses Array Destructuring</dt>\n                    <dd>useState proves a state variable and a setState function</dd>\n                    <dt>useState is a Hook</dt>\n                    <dd>useState is a hook baked in React</dd>\n                    <dt>Triggers Re-renders</dt>\n                    <dd>Like with props changes, changing the state of a component re-renders the whole component.</dd>\n                </ul>\n                <StateExample />\n            </div>\n        </div>\n    );\n};\n\nexport default State;\n\nfunction StateExample() {\n    const [text, setText] = useState('');\n    const [likeNum, setLikeNum] = useState(0);\n    const [textColor, setTextColor] = useState('blue');\n\n    return(\n        <div>\n            <input value={text} onChange={e => setText(e.target.value)}/>\n            <br/>\n            <img style ={{ width: '100px', height: '100px' }} src=\"https://upload.wikimedia.org/wikipedia/commons/1/13/Facebook_like_thumb.png\" onClick={e => setLikeNum(likeNum + 1)}/>\n            <span>{likeNum}</span>\n            <br/>\n            <br/>\n            <br/>\n            <br/>\n            <p style={{ color: textColor }} onMouseEnter={e => setTextColor('red')} onMouseLeave={e => setTextColor('blue')}>THE MOUSE IS BLUE</p>\n        </div>\n    );\n};","/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/concepts/Effects.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/concepts/PropsDemo.js",["92","93","94"],"import React, { useState } from 'react';\nimport PropTypes from 'prop-types';\n\nconst PropsDemo = () => {\n const [color, setColor] = useState('white');\n const [backgroundColor, setBackgroundColor] = useState('purple');\n const [borderRadius, setBorderRadius] = useState('5px');\n const [borderStyle, setBorderStyle] = useState('dashed');\n const [display, setDisplay] = useState('inline-block');\n const [width, setWidth] = useState('350px');\n const [textAlign, setTextAlign] = useState('center');\n\n let styles = {\n  color: color,\n  backgroundColor: backgroundColor,\n  borderRadius: borderRadius,\n  borderStyle: borderStyle,\n  display: display,\n  width: width,\n  textAlign: textAlign\n }\n\n const toggleColor = () => {\n  color === 'white' ? setColor('pink') : setColor('white');\n }\n\n const toggleBackgroundColor = () => {\n  backgroundColor === 'purple' ? setBackgroundColor('#191970') : setBackgroundColor('purple');\n }\n\n const toggleBorderRadius = () => {\n  borderRadius === '5px' ? setBorderRadius('20px') : setBorderRadius('5px');\n }\n\n const toggleBorderStyle = () => {\n  borderStyle === 'dashed' ? setBorderStyle('double') : setBorderStyle('dashed');\n }\n\n return (\n  <div className=\"main\">\n   <div className=\"mainDiv\">\n    <div style={styles}>\n     <FunctionalComponent\n      string=\"Will this display?\"\n      function={toggleColor}\n      selectedStyle={color}\n     />\n     <FunctionalComponent\n      string=\"Props are pretty cool, right?!\"\n      function={toggleBackgroundColor}\n      selectedStyle={backgroundColor}\n     />\n     <FunctionalComponent\n      string=\"You can send data from one component ...\"\n      function={toggleBorderRadius}\n      selectedStyle={borderRadius}\n     />\n     <FunctionalComponent\n      string=\"... to another!\"\n      function={toggleBorderStyle}\n      selectedStyle={borderStyle}\n     />\n    </div>\n   </div>\n  </div>\n );\n};\n\nexport default PropsDemo;\n\nconst FunctionalComponent = (props) => {\n return (\n  <div>\n   <p>{props.string}</p>\n   <button onClick={props.function}>Press Me!</button>\n   <TinyComponent selectedStyle={props.selectedStyle} />\n  </div>\n );\n};\n\nconst TinyComponent = (props) => {\n return (\n  <div>\n   <p>The current style: {props.selectedStyle} </p>\n  </div>\n );\n};\n\nFunctionalComponent.defaultProps = {\n string: 'This is wild!',\n function: () => console.log('Can I see this in my dev tools?'),\n selectedStyle: 'what style??'\n};\n\nFunctionalComponent.propTypes = {\n string: PropTypes.string.isRequired,\n function: PropTypes.func.isRequired,\n selectedStyle: PropTypes.string.isRequired\n};","/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/concepts/Hooks.js",["95"],"import React, { useState, useEffect } from 'react';\n\nconst Hooks = () => {\n const [query, setQuery] = useState('');\n const [results, setResults] = useState({});\n\n const fetcher = () => {\n  fetch(`https://swapi.co/api/people/${query}`)\n   .then(res => res.json())\n   .then(json => {\n    console.log(json)\n    setResults(json);\n   })\n }\n\n return (\n  <div className=\"main\">\n   <div className=\"mainDiv\">\n    <input value={query} onChange={e => setQuery(e.target.value)} placeholder=\"enter your star wars character number\" />\n    <button onClick={() => fetcher()}>Click for Character!</button>\n    {results ? <h2>{results.name}</h2> : <div></div>}\n   </div>\n  </div>\n );\n};\n\n\nconst Hooks2 = () => {\n const [results, queryNum, setQueryNum] = useNumHook('');\n const [clicks, setClicks] = useClicks(0);\n\n return(\n  <div className=\"main\">\n   <div className=\"mainDiv\">\n    <h3>Enter a number below to see a number fact.</h3>\n    <input value={queryNum} onChange={e => setQueryNum(e.target.value)} placeholder=\"enter a number\" />\n    {results ? <h2>{results}</h2> : <div></div>}\n    <button onClick={() => setClicks(clicks + 1)}>Click to update document title</button>\n   </div>\n  </div>\n );\n};\n\nconst useNumHook = (num) => {\n const [queryNum, setQueryNum] = useState(num);\n const [results, setResults] = useState('');\n\n useEffect(() => {\n  if(queryNum !== ''){\n   fetch(`http://numbersapi.com/${queryNum}`)\n    .then(res => res.text())\n    .then(json => {\n     console.log(json);\n     setResults(json);\n    });\n  };\n }, [queryNum]);\n\n return [results, queryNum, setQueryNum];\n};\n\nconst useClicks = (initCount) => {\n const [clicks, setClicks] = useState(initCount);\n\n useEffect(() => {\n  document.title = `You have clicked ${clicks} times`;\n }, [clicks])\n return [clicks, setClicks]\n}\n\nexport default Hooks2;","/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/apps/src/components/apps/timer-apps/TimePieceApp.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/apps/src/components/apps/timer-apps/TimerApp.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/apps/src/components/apps/timer-apps/ClockApp.js",[],"/Users/tyramsey/Downloads/react_class/react_intro/react-modules/src/components/apps/src/components/apps/timer-apps/StopWatchApp.js",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":11,"column":24,"nodeType":"102","endLine":11,"endColumn":96},{"ruleId":"100","severity":1,"message":"101","line":16,"column":24,"nodeType":"102","endLine":16,"endColumn":89},{"ruleId":"100","severity":1,"message":"101","line":21,"column":24,"nodeType":"102","endLine":21,"endColumn":92},{"ruleId":"100","severity":1,"message":"101","line":26,"column":24,"nodeType":"102","endLine":26,"endColumn":110},{"ruleId":"100","severity":1,"message":"101","line":31,"column":24,"nodeType":"102","endLine":31,"endColumn":106},{"ruleId":"100","severity":1,"message":"101","line":36,"column":24,"nodeType":"102","endLine":36,"endColumn":80},{"ruleId":"100","severity":1,"message":"101","line":41,"column":24,"nodeType":"102","endLine":41,"endColumn":99},{"ruleId":"100","severity":1,"message":"101","line":46,"column":24,"nodeType":"102","endLine":46,"endColumn":112},{"ruleId":"100","severity":1,"message":"101","line":51,"column":24,"nodeType":"102","endLine":51,"endColumn":96},{"ruleId":"100","severity":1,"message":"101","line":56,"colum